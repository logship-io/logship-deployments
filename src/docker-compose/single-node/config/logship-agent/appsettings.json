{
  "Output": {
    "endpoint": "http://127.0.0.1:5000",
    "interval": "00:00:01",
    "maximumBufferSize": 40000,
    "maximumFlushSize": 1000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "Inputs": [
    {
      "type": "HealthService",
      "interval": "00:00:05.000"
    },
    {
      "type": "JournalCtl",
      "flags": 0,
      "columns": [ "USERID" ]
    },
    {
      "type": "filesystem",
      "interval": "00:00:05"
    },
    {
      "type": "system",
      "interval": "01:00:00"
    },
    {
      "type": "network",
      "interval": "00:00:15"
    },
    {
      "type": "proc",
      "interval": "00:00:5"
    },
    {
      "type": "proc.openfiles",
      "interval": "00:05:00"
    },
    {
      "type": "udp",
      "port": 49999
    },
    {
      "type": "processes",
      "interval": "00:00:30"
    }
  ]
}
