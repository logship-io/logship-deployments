{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "endpoints": [
      "nettcp://0.0.0.0:6000"
    ],
    "masters": [
      "nettcp://127.0.0.1:6000"
    ],
    "master": {
      "enabled": true
    },
    "services": {
      "keyvalue": {
        "enable": true,
        "dataDirectory": "/logship/kvs"
      },
      "transactionManager": {
        "enable": true
      },
      "transactionLog": {
        "enable": true
      },
      "gateway": {
        "enable": true
      }
    }
  }
  